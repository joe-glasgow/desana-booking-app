    "@types/react-dom": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "expect": "^27.4.6",
    "mocha": "^9.2.0",
    "prettier": "^2.5.1",
    "sinon": "^13.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.6"
  },
