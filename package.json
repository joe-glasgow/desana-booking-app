  "version": "1.0.0",
  "description": "A task for Desana to calculate when a space is available depending on specific criteria.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --colors -r ts-node/register 'src/**/*.spec.{js,jsx,ts,tsx}'",
    "test:watch": "mocha -r ts-node/register --watch --watch-files src, src/**/*.spec.{js,jsx,ts,tsx}",
